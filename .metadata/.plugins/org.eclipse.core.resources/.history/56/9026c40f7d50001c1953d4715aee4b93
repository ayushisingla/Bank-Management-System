/*
 * package com.ayushi.authservice.restclients;
 * 
 * import org.springframework.cloud.openfeign.FeignClient; import
 * org.springframework.web.bind.annotation.GetMapping; import
 * org.springframework.web.bind.annotation.PathVariable;
 * 
 * import com.ayushi.authservice.entity.Customer; import
 * com.ayushi.authservice.entity.Loan;
 * 
 * @FeignClient("GATEWAY-SERVICE") public interface LoanClient {
 * 
 * @GetMapping(value = "/loanapi/loan/{loanid}") Loan
 * getLoanDetails(@PathVariable("loanid") int loanid);
 * 
 * @GetMapping(value = "/profileapi/{accountNo}") Customer
 * profileupdate(@PathVariable("accountNo") int accountNo); }
 */